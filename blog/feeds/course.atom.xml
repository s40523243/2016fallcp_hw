ctx.fillStyle = 'black'
ctx.strokeStyle = "black"
ctx.arc(120, 200, 9, 0, 2*math.pi, False)
ctx.fill()
ctx.stroke()
ctx.closePath()

ctx.beginPath()
ctx.fillStyle = 'white'
ctx.font = "16px Arial"
ctx.fillText("3", 45, 85)
ctx.fill()
ctx.stroke()
ctx.closePath()

# 4
